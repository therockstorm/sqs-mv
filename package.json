  },
  "devDependencies": {
    "@types/jest-when": "^2.7.2",
    "eslint": "^7.18",
    "husky": "^4.3",
    "jest-when": "^3.1.0",
    "skripts": "^0.9",
    "typescript": "^4"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "skripts pre-commit"
    }
  }
}
