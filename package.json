{
  "name": "sqs-mv",
  "version": "1.0.0",
  "scripts": {
    "analyze": "npm run lint && npm run test --coverage && npm run build",
    "build": "sls package",
    "clean": "rm -rf .serverless .webpack coverage",
    "deploy": "sls deploy",
    "format": "skripts format",
